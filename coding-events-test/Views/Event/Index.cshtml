@{
    Layout = null;
    ViewBag.Title = "Events";
}

<h1>All Events</h1>

<ul>
    @for(int i = 0; i < ViewBag.Events.Count; i++)
    {
       <li>@ViewBag.Events[i]</li>
    }
</ul>